<launch>
    <!-- Run the map server with a blank map -->
    <node pkg="map_server" type="map_server" name="map_server" args="$(find tb_nav)/maps/blank_map.yaml" />

    <include file="$(find tb_nav)/launch/fake_move_base.launch" />

    <!-- Run a static transform between /odom and /map -->
    <node pkg="tf" type="static_transform_publisher" name="odom_map_broadcaster" args="0 0 0 0 0 0 /odom /map 100" />
</launch>